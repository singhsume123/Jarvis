<p id="notice"><%= notice %></p>

<h1>Listing Student Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>School level</th>
      <th>School name</th>
      <th>Team name</th>
      <th>Pay code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_users.each do |student_user| %>
      <tr>
        <td><%= student_user.first_name %></td>
        <td><%= student_user.last_name %></td>
        <td><%= student_user.school_level %></td>
        <td><%= student_user.school_name %></td>
        <td><%= student_user.team_name %></td>
        <td><%= student_user.pay_code %></td>
        <td><%= link_to 'Show', student_user %></td>
        <td><%= link_to 'Edit', edit_student_user_path(student_user) %></td>
        <td><%= link_to 'Destroy', student_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student user', new_student_user_path %>
